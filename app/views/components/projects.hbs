{{#if resume.projects.length}}
<div class="container projects-container">
  {{> title value="Projet"}}

  {{#each resume.projects}}
  <section class="item">
    {{> section-header name=this.name }}
    {{#if position}}
      <h5>{{position}}</h5>
    {{/if}}

    {{#if description}}
      <p class="summary">{{description}}</p>
    {{/if}}

    {{#if highlights.length}}
      <ul>
        {{#each highlights}}
        <li>{{.}}</li>
        {{/each}}
      </ul>
    {{/if}}
  </section>
  {{/each}}
</div>
{{/if}}
